<body>
<div class="jumbotron text-center">
  <h1>Super Chat</h1>
</div>

<div class="container">
  <div class="row">
    <div class="col-sm-4">
      <table class="table table-striped">
        <table class="table table-striped">
          <thead>
            <tr>
              <th><h3 class="message">Available Chat Rooms</h3></th>
            </tr>
          </thead>
          <tbody>
            {{#each chatRooms as |chatRoom|}}
            <tr>
              <th><a href="/{{chatRoom}}">{{chatRoom}}</a></th>
            </tr>
            {{/each}}
          </tbody>
        </table>
    </div>
  </div>
</div>
</body>

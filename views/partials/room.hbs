<hr class="room-divider">
<section class="chat-room room-select clickable {{#if this.active}}active{{/if}}" id="room-{{this.slug}}">
  <p class="chat-room-element">
    <b class="chat-room-element">{{this.name}}</b>
  </p>
  <small class="chat-room-element" id="member-count-{{this.slug}}">{{this.memberAmount}} Members</small>
</section>
